{
  "name": "@golevelup/nestjs-google-cloud-pubsub",
  "version": "1.1.0",
  "description": "Google cloud pubsub wrapper for NestJS",
  "author": "Nikita Chaikin <nikita.chaikinn@gmail.com>",
  "homepage": "https://github.com/golevelup/nestjs/blob/master/packages/google-cloud-pubsub/README.md",
  "license": "MIT",
  "keywords": [
    "NestJS",
    "Google Cloud",
    "PubSub",
    "gcp"
  ],
  "exports": {
    "import": {
      "types": "./lib/index.d.mts",
      "default": "./lib/index.mjs"
    },
    "require": {
      "types": "./lib/index.d.cts",
      "default": "./lib/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/golevelup/nestjs.git"
  },
  "scripts": {
    "copy-readme": "cp ../../docs/modules/google-pubsub.md ./README.md",
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/golevelup/nestjs/issues"
  },
  "dependencies": {
    "@golevelup/nestjs-discovery": "workspace:^"
  },
  "devDependencies": {
    "avsc": "^5.7.9",
    "@google-cloud/pubsub": "^5.2.0",
    "@protobuf-ts/runtime": "^2.11.1"
  },
  "peerDependencies": {
    "@google-cloud/pubsub": "^5.2.0",
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "avsc": "^5.7.9",
    "@protobuf-ts/runtime": "^2.11.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
