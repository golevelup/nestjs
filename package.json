{
  "name": "golevelup",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "commit": "npm run git-cz",
    "publish": "npm run build && npm run test && lerna publish -m 'chore: publish'",
    "publish:prerelease": "npm run build && npm run test && lerna publish prerelease --preid rc -m 'chore: publish prerelease'",
    "build": " nx run-many --target=build --all",
    "test": "nx run-many --target=test --all --parallel",
    "test:integration": "nx run-many --target=integration --all",
    "lint": "nx run-many --target=lint --all --parallel"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/prompt": "^16.1.0",
    "@nestjs/testing": "^8.2.6",
    "@nrwl/cli": "13.4.6",
    "@nrwl/eslint-plugin-nx": "13.4.6",
    "@nrwl/jest": "13.4.6",
    "@nrwl/js": "13.4.6",
    "@nrwl/linter": "13.4.6",
    "@nrwl/node": "13.4.6",
    "@nrwl/tao": "13.4.6",
    "@nrwl/workspace": "13.4.6",
    "@types/amqplib": "^0.8.2",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.178",
    "@types/node": "16.11.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.2.2",
    "prettier": "^2.3.1",
    "supertest": "^6.2.2",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3",
    "zod": "^3.11.6"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/microservices": "^8.2.6",
    "@nestjs/platform-express": "^8.2.6",
    "amqp-connection-manager": "^4.0.1",
    "amqplib": "^0.8.0",
    "graphql-request": "^3.7.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2",
    "shortid": "^2.2.16",
    "stripe": "^8.199.0",
    "tslib": "^2.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{md,yml,html,css,scss,json}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
